

<p id="notice"><%= notice %></p>

<h1>Requests</h1>

<table>
  <thead>
    <tr>
      <th>Request</th>
      <th>Hours</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requests.each do |attendance_log| %>
      <tr>
        <td><%= attendance_log.event_id %></td>
        <td><%= attendance_log.user_id %></td>
        <td><%= link_to 'Show', attendance_log %></td>
        <td><%= link_to 'Edit', edit_attendance_log_path(attendance_log) %></td>
        <td><%= link_to 'Destroy', attendance_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

